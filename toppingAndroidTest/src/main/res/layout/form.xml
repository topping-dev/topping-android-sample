<?xml version="1.0" encoding="utf-8"?>
<LinearLayout 
    lua:id="formTestLL" 
    android:id="@+id/formTestLL" 
    xmlns:android="http://schemas.android.com/apk/res/android" 
    xmlns:lua="http://schemas.android.com/apk/res-auto" 
    android:layout_width="fill_parent" 
    android:layout_height="fill_parent" 
    android:orientation="vertical" 
    android:background="#7993AB">
	<LGAutoCompleteTextView
	    android:id="@+id/formTestAutoCompTV"
	    android:layout_width="fill_parent"
    	android:layout_height="wrap_content">
	</LGAutoCompleteTextView>
	<LGButton
	    lua:id="formTestButton"
	    android:layout_width="fill_parent"
    	android:layout_height="wrap_content"
    	android:text="Button Text">
	</LGButton>
	<LGCheckBox
	    lua:id="formTestCheckBox"
	    android:layout_width="fill_parent"
    	android:layout_height="wrap_content"
    	android:text="Checkbox text">
	</LGCheckBox>
	<LGComboBox
	    lua:id="formTestComboBox"
	    android:layout_width="fill_parent"
    	android:layout_height="wrap_content">
	</LGComboBox>
	<LGEditText
	    lua:id="formTestEt"
	    android:layout_width="fill_parent"
    	android:layout_height="wrap_content">
	</LGEditText>
	<LGProgressBar
	    lua:id="formTestProgressBar"
	    android:layout_width="fill_parent"
    	android:layout_height="wrap_content"
		style="@style/Widget.AppCompat.ProgressBar.Horizontal"
		android:max="100">
	</LGProgressBar>
	<LGImageView
	    lua:id="formTestIv"
	    android:src="@drawable/ic_launcher"
	    android:layout_width="fill_parent"
    	android:layout_height="wrap_content">
	</LGImageView>
</LinearLayout>